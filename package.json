{
  "name": "basspj-blog",
  "private": true,
  "description": "based on gatsby-starter-blog@429393dcc8eaab635d17ffe3b581ddaa0738b56a and overreacted.io",
  "version": "1.0.0",
  "author": "basspj <thanapol.pj@gmail.com>",
  "bugs": {
    "url": "https://github.com/basspj/basspj.github.io/issues"
  },
  "scripts": {
    "dev": "gatsby develop",
    "prebuild": "shx cp README.md public",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "gh-pages": "gh-pages -d public -b master",
    "deploy": "gatsby build && yarn gh-pages",
    "format": "prettier --write {gatsby-*.js,src/**/*.{js,ts,tsx,json,css}}",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "commit": "git-cz"
  },
  "dependencies": {
    "gatsby": "^2.1.31",
    "gatsby-image": "^2.0.33",
    "gatsby-plugin-feed": "^2.0.15",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.0.28",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.9",
    "gatsby-remark-copy-linked-files": "^2.0.10",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.5",
    "gatsby-remark-responsive-iframe": "^2.0.10",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-remark": "^2.3.2",
    "gatsby-transformer-sharp": "^2.1.17",
    "prismjs": "^1.15.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-infinite-scroll-component": "4.5.2",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/node": "11.11.3",
    "@types/prismjs": "1.9.1",
    "@types/react": "16.8.8",
    "@types/react-helmet": "5.0.8",
    "@types/typography": "0.16.2",
    "@typescript-eslint/parser": "1.4.2",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-react": "7.11.1",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "^1.16.4",
    "shx": "0.3.2",
    "ts-node": "8.0.1",
    "tsconfig-paths": "3.7.0",
    "tsutils": "3.7.0",
    "typescript": "3.2.4"
  },
  "homepage": "https://basspj.github.io/",
  "license": "MIT",
  "main": "n/a",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,ts,tsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
